{
  "name": "sumocoders/framework-search-bundle",
  "type": "symfony-bundle",
  "description": "Search bundle for our mini-framework",
  "keywords": ["Search", "SumoCoders"],
  "license": "MIT",
  "authors": [
    {
      "name": "Tijs Verkoyen",
      "email": "tijs@sumocoders.be"
    }
  ],
  "prefer-stable": true,
  "require": {
    "php": "^5.5 || ^7.0",

    "symfony/event-dispatcher": "^2.8",
    "doctrine/orm": "^2.5"
  },
  "require-dev": {
    "symfony/form": "^2.8",
    "symfony/validator": "^2.8",
    "mopa/bootstrap-bundle": "^3.0",
    "symfony/phpunit-bridge": "^3.0",
    "phpunit/phpunit": "^5.2 || ^4.8"
  },
  "autoload": {
    "psr-0": { "SumoCoders\\FrameworkSearchBundle": "" }
  },
  "target-dir": "SumoCoders/FrameworkSearchBundle",
  "config": {
    "bin-dir": "bin"
  }
}
